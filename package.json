{
  "name": "awc-ui",
  "version": "1.0.0",
  "description": "front end for awc",
  "main": "./public/index.js",
  "scripts": {
    "build": "npm run build:html; npm run build:css; npm run build:js",
    "build:html": "cp ./src/ui/index.html ./public/index.html",
    "build:css": "tailwindcss -i ./src/ui/index.css -o ./public/index.css",
    "build:js": "webpack",
    "dev": "cargo watch -i 'public/*' -x 'run --bin awc --no-default-features --features http'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EverlastingBugstopper/awc.git"
  },
  "author": "Avery Harnish <averyharnish@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EverlastingBugstopper/awc/issues"
  },
  "homepage": "https://github.com/EverlastingBugstopper/awc#readme",
  "devDependencies": {
    "@swc/core": "^1.2.215",
    "@tailwindcss/typography": "^0.5.4",
    "browserslist": "^4.21.2",
    "daisyui": "^2.19.0",
    "source-map-loader": "^4.0.0",
    "swc-loader": "^0.2.3",
    "tailwindcss": "^3.1.6",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
